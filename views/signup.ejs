<%- include('partials/header') %>
</head>
<body class="text-centre">
  <form action="/signup" method="post" class="form-signin">
      <h1 class="h3 mb-3 font-weight-normal" id="signtext">SignUp</h1>
      <label for="inputEmail" class="sr-only">Email address</label>
      <input type="email"  name="email" class="form-control top" placeholder="Email address" required autofocus>
      <input type="text"  name="name" class="form-control middle" placeholder="Name" required >
      <input type="text"  name="aadhar" class="form-control bottom" placeholder="Aadhar Number" required>
      <input type="number"  name="phone" class="form-control bottom" placeholder="Phone Number" required>
      <input type="password"  name="password" class="form-control bottom" placeholder="Password" required>
      <div class="">
          <button type="submit" name="button" class="btn btn-dark formbutton">Submit</button>
      </div>
      </form>
